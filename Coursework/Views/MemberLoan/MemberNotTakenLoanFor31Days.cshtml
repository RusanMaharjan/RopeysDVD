@model System.Collections.Generic.List<Coursework.Models.DTO.MemberNotTakenLoanDTO>

@{
    ViewBag.Title = "Member Not taken loan for 31 days";
    Layout = "_Layout";
}

<h2>Member Who have not Taken Loan For 31 Days</h2>
<table>
    <thead>
    <tr>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Address</th>
        <th>Title</th>
        <th>Last Date of Loan</th>
        <th>Days Since Last Loan</th>
    </tr>
    </thead>
    
    <tbody>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.firstName</td>
            <td>@item.lastName</td>
            <td>@item.address</td>
            <td>@item.DvdTitle</td>
            <td>@item.dateOut</td>
            <td>@item.numberOfDays</td>
        </tr>
    }
    </tbody>
</table>
